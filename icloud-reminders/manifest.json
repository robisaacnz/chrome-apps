{
  "name":"Reminders",
  "description":"Access your iCloud reminders.",
  "version":"1.2",
  "update_url":"http://clients2.google.com/service/update2/crx",
  "manifest_version":2,
  "app":{
    "urls":[
      "https://www.icloud.com/#reminders"
    ],
    "launch":{
      "web_url":"https://www.icloud.com/#reminders",
      "container":"panel",
      "width":1000,
      "height":650
    }
  },
  "icons":{
    "16":"icon-16.png",
    "128":"icon-128.png"
  },
  "permissions":[
    "clipboardRead",
    "clipboardWrite",
    "notifications"
  ]
}